<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->

        <item name="android:actionBarStyle">@style/AppTheme.MyActionBar</item>
        <item name="android:windowBackground">@drawable/black_back</item>
    </style>

    <style name="AppTheme.MyActionBar" parent="@style/Widget.AppCompat.ActionBar.Solid">
        <item name="android:background">#FF346266</item>
        <item name="android:titleTextStyle">@style/AppTheme.ActionBar.TitleTextStyle</item>
    </style>

    <style name="AppTheme.ActionBar.TitleTextStyle" parent="@style/TextAppearance.AppCompat.Widget.ActionBar.Title">

        <item name="android:textColor">#FFFFFF</item>

    </style>


</resources>

